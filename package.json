{
    "name": "web-minecraft",
    "version": "1.0.0",
    "description": "Minecraft client written in Javascript",
    "scripts": {
        "start": "node index.js production",
        "dev": "npm run prebuild && node index.js development",
        "build": "cd src/client && webpack --config webpack.prod.js",
        "prebuild": "node src/prebuild.js",
        "clean": "rimraf src/client/dist/*",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "postinstall": "node src/postinstall.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint:fix"
        }
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/michaljaz/web-minecraft.git"
    },
    "keywords": [
        "minecraft",
        "game",
        "browser",
        "mineflayer",
        "threejs"
    ],
    "dependencies": {
        "@msgpack/msgpack": "^2.4.1",
        "@tweenjs/tween.js": "^18.6.4",
        "ansi-to-html": "^0.6.14",
        "bootstrap": "^4.6.0",
        "canvas": "^2.6.1",
        "compression": "^1.7.4",
        "dat.gui": "^0.7.7",
        "express": "^4.17.1",
        "extract-zip": "^2.0.1",
        "helmet": "^4.4.1",
        "html-webpack-plugin": "^4.5.1",
        "http": "0.0.1-security",
        "jquery": "^3.5.1",
        "lodash-webpack-plugin": "^0.11.6",
        "mineflayer": "^3.0.0",
        "node": "^14.15.2",
        "open": "^7.4.2",
        "popper.js": "^1.16.1",
        "prismarine-block": "^1.7.3",
        "prismarine-chunk": "^1.22.0",
        "rimraf": "^3.0.2",
        "stats-js": "^1.0.1",
        "sweetalert": "^2.1.2",
        "three": "^0.125.0",
        "webpack-dev-server": "^3.11.1",
        "webpack-merge": "^5.7.3",
        "webpackbar": "^5.0.0-3",
        "worker-loader": "^3.0.7",
        "ws": "^7.4.3"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^6.3.1",
        "css-loader": "^5.0.1",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "file-loader": "^6.2.0",
        "husky": "^1.0.0-rc.13",
        "node-sass": "^5.0.0",
        "prettier": "^2.2.1",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.2.0"
    }
}
